<%= simple_form_for @book,:html =>{ :multipart => true} do |f| %>
<%= select_tag(:category_id,options_for_select(@category), :prompt => "select category")%>
 <%= f.file_field :book_img  %>
  <%= f.input :title  %>
  <%= f.input :description %>
  <%= f.input :author %>
  <%= f.button :submit , class: "btn-custom2" %>
<% end %>